#ticketsContent
	div.letterbox
		h1.myriad.black!= event.Name;
	section.clearfix
		input#venueId(type="hidden",value=event.VenueID)
		input#eventName(type="hidden",value=event.Name)
		#ticketsContainer.leftBlock
			- if (typeof tickets != "undefined")
				- each ticket in tickets
					.ticketDetails!= partial('includes/ticket-list',{ticket: ticket})
			- else
				#noTickets No tickets available for this event.
		#fvmap
		#thumbnail
