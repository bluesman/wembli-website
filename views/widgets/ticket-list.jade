//ticketnetwork settings
- var brokerId = '5006'
- var siteNumber = '0'
- var tnUrl = 'https://tickettransaction2.com/Checkout.aspx?brokerid='+brokerId+'&sitenumber='+siteNumber;
// ----
- var cnt = 0;
ul
	- each ticket in tickets
		- cnt++;
		- if (cnt > 50) 
			- break;
		- var ticketLink = tnUrl+'&tgid='+ticket.ID+'&evtid='+ticket.EventID+'&price='+ticket.ActualPrice+'&treq='+ticket.TicketQuantity+'&SessionId='+ticketSessionId()
		- var qtyStr = ticket.TicketQuantity
		- qtyStr += (ticket.TicketQuantity > 1) ? " tickets " : " ticket "
		li(id='ticket-id-'+ticket.ID)
			p.info 
				a(href=ticketLink)= "Section: "+ticket.Section + " - Row: " + ticket.Row
				span.seats.block!= (ticket.LowSeat == '*') ? "" : "Seats: "+ticket.LowSeat + '-' + ticket.HighSeat 
				span.rating.block
					em Wembli Rating:
					span.stars!= (ticket.Rating ? ticket.Rating : 0)
					br
					em.ratingDescription!= ticket.RatingDescription
			p.cta
				a.btn.btn-primary(href=ticketLink) Add to Plan
			span.ticket-price-box
				span.ticket-quantity!= qtyStr
				span.ticket-price!= '$'+ticket.ActualPrice
				span (each)